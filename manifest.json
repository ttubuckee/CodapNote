{
  "author": "daehyun-kim",
  "manifest_version":2,
  "name": "프로그래머스 타이머",
  "version": "0.0.2",
  "description":"timer for programmers user",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "browser_action" : {
    "default_popup" : "./popup.html",
    "default_icon" : "/src/img/timer_icon.png"
  },
  "web_accessible_resources": [
    "/src/img/*.png"
  ],
  "content_scripts" : [{
    "matches": ["https://programmers.co.kr/learn/courses/30/lessons/*"],
    "js": ["./src/js/content_scripts.js"]
  }]
}